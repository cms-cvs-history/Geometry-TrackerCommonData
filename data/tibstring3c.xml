<?xml version="1.0"?>
<DDDefinition xmlns="http://www.cern.ch/cms/DDL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.cern.ch/cms/DDL ../../../DetectorDescription/Schema/DDLSchema.xsd">

<ConstantsSection label="tibstring3c.xml" eval="true">
 <Constant name="StringLoMinL" value="66.10*cm"/>
 <Constant name="StringUpMinL" value="70.10*cm"/>
 <Constant name="StringLoPlsL" value="[tibstringpar:StringL]-[StringLoMinL]"/>
 <Constant name="StringUpPlsL" value="[tibstringpar:StringL]-[StringUpMinL]"/>
 <Constant name="WaferZLoOffset" value="+[tibmodpar:WaferZ]"/>  <!-- Offset due to WaferZ for INT strings -->
 <Constant name="WaferZUpOffset" value="-[tibmodpar:WaferZ]"/>  <!-- Offset due to WaferZ for EXT strings -->
 <Constant name="ICCLoOffset" value="+[tibmodpar:ModuleDz]+[tibstringpar:ICCBoxL]/2"/>  
 <!-- Offset of ICC Box for INT strings -->
 <Constant name="ICCUpOffset" value="-[tibmodpar:ModuleDz]-[tibstringpar:ICCBoxL]/2"/>  
 <!-- Offset of ICC Box for EXT strings -->
</ConstantsSection>
 
</DDDefinition>
